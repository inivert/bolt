{
  "root": true,
  "extends": ["@nuxtjs/eslint-config-typescript"],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": {
      "ts": "@typescript-eslint/parser",
      "js": "@typescript-eslint/parser",
      "<template>": "espree"
    }
  },
  "rules": {
    "vue/multi-word-component-names": "off"
  }
} 